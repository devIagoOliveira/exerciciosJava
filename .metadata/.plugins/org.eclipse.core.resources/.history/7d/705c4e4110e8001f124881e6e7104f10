package exercicio03;

import java.util.Scanner;

public class ex05 {

	public static void main(String[] args) {
		
		double valorTotal;
		Scanner sc = new Scanner (System.in);
		
		int codigo, qtd;
		
		codigo = sc.nextInt();
		qtd = sc.nextInt();
		
		if (codigo == 1) {
			valorTotal = qtd * 4.00;
			System.out.printf("Total: R$ %.2f%n", valorTotal);
		} else if (codigo == 2) {
			valorTotal = qtd * 4.50;
			System.out.printf("Total: R$ %.2f%n", valorTotal);
		}
		
		sc.close();

	}

}
